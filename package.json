{
  "name": "vscode-devdocs",
  "displayName": "vscode-devdocs",
  "description": "Devdocs documentation browser for VSCode editor",
  "version": "0.1.0",
  "icon": "devdocs.png",
  "publisher": "benhjt",
  "engines": {
    "vscode": "^1.5.0"
  },
  "author": "benhjt",
  "license": "MIT",
  "homepage": "https://github.com/benhjt/vscode-devdocs/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/benhjt/vscode-devdocs.git"
  },
  "bugs": {
    "url": "https://github.com/benhjt/vscode-devdocs/issues"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.devDocs"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [{
      "command": "extension.devDocs",
      "title": "Search dev docs"
    }]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "vscode": "^0.11.0"
  }
}
